{
  "manifest_version": 3,
  "name": "LTTStore.com Real Price ",
  "description": "This Extention take the price of items on LTTStore.com and add tax and shipping to the prices.",
  "version": "1.0",
  "permissions": ["storage", "scripting", "tabs"],
  "host_permissions": [
    "*://www.lttstore.com/*"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "popup/options.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
